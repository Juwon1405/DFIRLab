{
  "builders": [
    {
      "type": "qemu",
      "communicator": "winrm",
      "winrm_port": "5985",
      "skip_compaction": "false",
      "output_directory": "../ova/PACKER-cuckooVM/win10",
      "format": "qcow2",
      "disk_size": "20480",
      "accelerator": "kvm",
      "cpus":"2",
      "memory":"1024",
      "qemuargs": [
        [
          "-drive",
          "file=../ova/PACKER-cuckooVM/win10/{{ .Name }},if=virtio,cache=writeback,discard=ignore,format=qcow2,index=1"
        ],
        [
          "-drive",
          "file=../ISO--drivers/virtio-win-0.1.185.iso,media=cdrom,index=3"
        ],
        [
          "-m",
          "1024"
        ],
        [
          "-usbdevice",
          "tablet"
        ],
        [
          "-smp",
          "2"
        ],
        [
          "-cpu",
          "Westmere"
        ]
      ],
      "vm_name": "Win10.qcow2",
      "iso_url": "../ISO/win10_19041.264.200511-0456.vb_release_svc_refresh_CLIENTENTERPRISEEVAL_OEMRET_x64FRE_en-us.iso",
      "iso_checksum": "md5:e85637e135e9b6dbc5fa02b463a95764",
      "boot_wait": "6m",
      "winrm_username": "analyste",
      "winrm_password": "analyste",
      "winrm_timeout": "2h",
      "shutdown_command": "shutdown /s /t 10 /f /d p:4:1 /c \"Packer shutdown\"",
      "shutdown_timeout": "30m",
      "floppy_files": [
        "./Autounattend.xml",
        "../SCRIPTS/disable-windows-update.ps1",
        "../SCRIPTS/fixnetwork.ps1",
        "../SCRIPTS/disable-winrm.ps1",
        "../SCRIPTS/enable-winrm.ps1",
        "../SCRIPTS/chocolatey.ps1",
        "../SCRIPTS/disable-windowsDefender.bat"
      ],
      "headless": false
    }
  ],
  "provisioners": [
    {
      "type": "powershell",
      "inline": [
        "Set-NetFirewallProfile -Profile Domain,Public,Private -Enabled False",
        "choco install -y 7zip putty filezilla firefoxesr vlc vscode",
        "choco install -y git python2 python3 golang",
        "choco install -y adobereader"
      ]
    },
    {
      "type": "file",
      "source": "../FILES/cuckoo_agent.py",
      "destination": "C:\\ProgramData\\Microsoft\\Windows\\Start Menu\\Programs\\StartUP\\agent.pyw"
    }
  ]
}
